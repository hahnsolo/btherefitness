		<!-- Login section -->
		<section id="login" style="display:none;">
			<form name="Login" id="fLogin" autocomplete="on">
				<table>
					<tr>
						<td><label>Username:</label></td>
						<td>
							<!-- Username -->
							<input type="text" placeholder="Username" name="Name" id="lName">
						</td>
					</tr>
					<tr>
						<td><label>Password:</label></td>
						<td>
							<!-- Password -->
							<input type="password" placeholder="Password" name="Password" id="lPassword">
						</td>
					</tr>
					<tr>
						<td>
							<!-- Login button -->
							<input type="submit" id="lLogin" value="Login">
						</td>
						<td>
							<!-- Signup redirect -->
							<span id="lSignup" style="float:right; color:#0645AD;">Signup</span>
						</td>
					</tr>
				</table>
			</form>
		</section>
		
		<!-- Signup section -->
		<section id="signup" style="display:none;">
			<form name="Signup" id="fSignup" autocomplete="on">
				<table>
					<tr>
						<td><label>Email:</label></td>
						<td>
							<!-- Email -->
							<input type="email" placeholder="Email" name="Email" id="sEmail">
						</td>
					</tr>
					<tr>
						<td><label>Username:</label></td>
						<td>
							<!-- Username -->
