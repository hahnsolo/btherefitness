					</tr>
					<tr>
						<td><label>Password:</label></td>
						<td>
							<!-- Password -->
							<input type="password" placeholder="Password" name="Password" id="sPassword"><br>
						</td>
					</tr>
					<tr>
						<td>
							<!-- Submit button -->
							<input type="submit" id="sSignup" value="Sign Up" />
						</td>
						<td>
							<!-- Login redirect -->
							<span id="sLogin" style="float:right; color:#0645AD;">Login</span>
						</td>
					</tr>
				</table>
			</form>
		</section>
		
